{
    "manifest_version": 3,
    "name": "SuperBlocker: Block distracting URLs and focus on important tasks.",
    "version": "1.6",
    "permissions": [
      "declarativeNetRequest",
      "storage",
      "tabs"
    ],
    "background": {
      "service_worker": "background.fw.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/bk_icon.png"
      }
    }
  }